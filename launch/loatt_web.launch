<launch>

  <node name = "loatt_2l_web"
  	type = "relay.py"
    	pkg = "nasco_system">
    <remap from = '/web' to = '/loatt_2l_web'/>
    <remap from = '/value' to = '/loatt_2l' />
    <param name = "config_file"
           value = "$(find nasco_system)/relay_conf/web.conf" />
    <param name = "rate" value = "1." />
  </node>

